function SCRIPT_QOPR81_FAKESCRIPT() -- fly.Script 
	local script = Instance.new('Script')
	script.Parent = fly
		script.Parent.MouseButton1Click:connect(function()
		
		--Button Sound Effect
		local s = Instance.new("Sound")
		 
		s.Name = "Sound"
		s.SoundId = "http://www.roblox.com/asset/?id=452267918"
		s.Volume = 7
		s.Looped = false
		s.archivable = false
		 
		s.Parent = game.Workspace
		 
		s:play()
		
		script.Parent.Active = false
		script.Parent.BackgroundTransparency = 0.5
		script.Parent.Text = "LOADED"
		wait(2)
		script.Parent.Active = true
		script.Parent.BackgroundTransparency = 0.7
		script.Parent.Text = "TOGGLE FLY"
		
		local function playsound(id)
			local sound = Instance.new('Sound',Camera)
			sound.SoundId = 'rbxassetid://'..id
			sound.Volume = 3
			sound:Play()
			game:GetService('Debris'):AddItem(sound,5)
		end
		
		local function Notification(...)
			playsound(140910211)
			game.StarterGui:SetCore('SendNotification',...)
		end
		
		Notification({Title='Loaded';Text='Fly Toggled';Duration=4;Icon=''})
		
		--Fly Up Function
		running = true
		  spawn(function()
		        while running do
		            game.Players.LocalPlayer.Character.Humanoid:ChangeState(3);
		            print("network_id_invalid")
		            wait()
		        end
		        print("network_id_valid")
		    end)
		    
		    local function stopRunning()
		        running = false
		    end
		    
		    wait(2)
		    stopRunning()
		
		--Keep Flying Function
		keepFlying=true
		local uis = Game:GetService("UserInputService")
		uis.InputBegan:connect(function(inst)
		    if inst.KeyCode == Enum.KeyCode.F then
		          keepFlying=true
		      while keepFlying do
			wait(0.001)
		    game.Players.LocalPlayer.Character.Humanoid:ChangeState(10);
		      end
		    end
		end)
		uis.InputEnded:connect(function(inst)
		    if inst.KeyCode == Enum.KeyCode.F then
		        keepFlying=false
		    end
		end) 
			 
		
		script.Parent.Visible = false
		wait(0.3)
		script.Parent.Visible = true
	end)

end
