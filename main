function SCRIPT_IELP78_FAKESCRIPT() -- speed.Script 
	local script = Instance.new('Script')
	script.Parent = speed
	script.Parent.MouseButton1Click:connect(function()
		
		--Button Sound Effect
		local s = Instance.new("Sound")
		 
		s.Name = "Sound"
		s.SoundId = "http://www.roblox.com/asset/?id=452267918"
		s.Volume = 7
		s.Looped = false
		s.archivable = false
		 
		s.Parent = game.Workspace
		 
		s:play()
		
		local function playsound(id)
			local sound = Instance.new('Sound',Camera)
			sound.SoundId = 'rbxassetid://'..id
			sound.Volume = 3
			sound:Play()
			game:GetService('Debris'):AddItem(sound,5)
		end
		
		local function Notification(...)
			playsound(140910211)
			game.StarterGui:SetCore('SendNotification',...)
		end
		
		Notification({Title='Loaded';Text='Speed Toggled';Duration=4;Icon=''})
		    
		--Speed Function 
		keepGoing=true
		local uis = Game:GetService("UserInputService")
		uis.InputBegan:connect(function(inst)
		    if inst.KeyCode == Enum.KeyCode.LeftShift then
		          keepGoing=true
		      while keepGoing do
				  wait(0.16)
		         local player = game.Players.LocalPlayer
				 local Character = player.Character
				player.Character.HumanoidRootPart.CFrame = player.Character.HumanoidRootPart.CFrame + player.Character.HumanoidRootPart.CFrame.lookVector*1 --stud pos change
		      end
		    end
		end)
		uis.InputEnded:connect(function(inst)
		    if inst.KeyCode == Enum.KeyCode.LeftShift then
		        keepGoing=false
		    end
		end) 
		
		--One Time Use
		script.Parent.Active = false
		script.Parent.BackgroundTransparency = 0.5
		script.Parent.Text = "LOADED"
		
	end)

end
